{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-zA-Z]+(\\-?[a-zA-Z0-9]+)*$"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "license": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "edits": {
      "type": "string"
    },
    "editor": {
      "type": "string"
    },
    "creationDate": {
      "type": "string"
    },
    "jsonldContext": {
      "type": "object"
    },
    "dependsOn": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[a-zA-Z]+(\\-?[a-zA-Z0-9]+)*$"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "title",
    "description",
    "license",
    "author"
  ]
}
